{
  "request": {
    "urlPathPattern": "/.*/.*",
    "method": "POST",
    "headers": {
      "Accept": {
        "contains": "application/json"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "X-B3-TraceId": "{{request.headers.X-B3-TraceId}}"
    },
    "bodyFileName": "{{request.path.[0]}}/{{request.path.[1]}}.json",
    "transformers": [
      "response-template"
    ]
  }
}
